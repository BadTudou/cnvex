{
    "name": "bravist/cnvex",
    "description": "Cnvex is a PHP library for e.cnvex.cn",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^5.5.9 || ^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.1",
        "illuminate/log": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "license": "Apache2.0",
    "authors": [
        {
            "name": "程会勇",
            "email": "chenghuiyong1987@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bravist\\Cnvex\\": "src/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
     "extra": {
        "laravel": {
            "providers": [
                "Bravist\\Cnvex\\ServiceProvider"
            ]
        }
    }
}
